<div id="adminInfo">
    <h1>User Swaps List</h1><br>
<table class="table table-striped">
   
    <% 
    if (typeof list != 'undefined' && list != null && list.length > 0) { %>

  <tr>
  <th>User</th>
  <th>Item Name</th>
  <th>Description</th>
  <th id="del">Image</th>
  <th id="del">Delete</th>
  </tr>
     
      <%      list.forEach( function ( postData ){
                    %>

            <tr>             
                <td id="tData"><%= postData.user %></td>
                <td id="tData"><%= postData.title %></td>
                <td id="tData"><%= postData.desc %></td>
                <td id="tData"><img id="photoSmall" src="data:image/<%=postData.img.contentType%>;base64, <%=postData.img.data.toString('base64')%>"></td>

                <td id="tData">
                  <div>                                                                    
                      <a  id="itemId" href="/swaps/delete/<%=postData._id %>">
                            <i class="fa fa-trash fa-m" id="del" style="color:#f75063"                           
                             data-toggle="tooltip" data-placement="bottom" title="Delete Swap"></i></a>
                    </div>
                </td>                                                        
            </tr>  
            
<% });
} else { %>
            <tr><td colspan="3"> No Data </td></tr>
<% } %>


</table>

</div>
<div id="accBtn">
    <a href="/account" class="btn btn-info" >Back <i class="fas fa-backward"></i></a>
</div>