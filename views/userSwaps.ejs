<h1>Swaps List</h1>

<div id="userInfo">
   
<table class="table table-striped">
   
    <% 
    if (typeof list != 'undefined' && list != null && list.length > 0) {
     
            list.forEach( function ( swapData ){
                    %>

            <tr>
                <td id="tData"><%= swapData.title %></td>
                <td id="tData"><%= swapData.desc %></td> 
                <td id="tData"><img id="photoSmall" src="data:image/<%=swapData.img.contentType%>;base64, <%=swapData.img.data.toString('base64')%>"></td> 
                <td id="tData">
                    <div id ="editDelete">
                            <a href="/swaps/edit/<%=swapData._id %>"><i class="fa fa-pencil-square-o fa-lg" style="color:#14b7eb"
                                data-toggle="tooltip" data-placement="bottom" title="Edit swap"></i></a>
                    
                    
                            <a href="/swaps/delete/<%=swapData._id %>"><i class="fa fa-trash fa-lg" style="color:#f75063"
                                data-toggle="tooltip" data-placement="bottom" title="Delete swap" onclick="return confirm('Are you sure you want to delete?');"></i></a>
</div>
                        </td>          
                                        
            </tr>  
            
<% });
} else { %>
            <tr><td colspan="3"> No Data </td></tr>
<% } %>


</table>

</div>
<div id="accBtn">
    <a href="/account" class="btn btn-info" >Back <i class="fas fa-backward"></i></a>
</div>