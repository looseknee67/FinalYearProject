<div id="adminInfo">
    <h1>User Comments List</h1><br>
<table class="table table-striped">

    <% 
            
    if (typeof list != 'undefined' && list != null && list.length > 0) { %>
  <tr id="tData"> 
  <th>User</th>
  <th>User Comment</th>  
  <th id="del">Delete</th>
  </tr>
          <%  list.forEach( function ( commentData ){
                    %>

            <tr>
                <td id="tData"><%= commentData.user %></td>
                <td id="tData"><%= commentData.comment %></td>
                
            <td id="tData">
            <div>
                <a id="itemId" href="/comment/delete/<%=commentData._id %>"><i class="fa fa-trash fa-m" style="color:#f75063"
                data-toggle="tooltip" data-placement="bottom" title="Delete Comment"></i></a>
            </div>
            </td>                                                 
            </tr>  

            
<% });
} else { %>
            <tr><td colspan="3"> No Data </td></tr>
<% } %>


</table>
</div>
<div id="accBtn">
    <a href="/account" class="btn btn-info" >Back <i class="fas fa-backward"></i></a>
</div>

