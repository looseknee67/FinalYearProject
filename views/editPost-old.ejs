<div id = accMessages>

<h1>Edit Post</h1>
<h2><%= list.postId %></h2>
<h2 name="title"><%= list.title %></h2>
<h2><%= list.content %></h2>
<h2><%= list.id %></h2>


<form class="custom-form" action="/editpost/<%= list.id %>" ?_method="PUT" method="POST">
<a href="/editpost/<%= list._id %>">Update</a>

<div>
    <label for="title">Title</label>
    <input type="text" class="form-control" name="title" style="color: black" value= "<%= list.title %>" ></input>
</div>

<div>
    <label for="content">Content</label>
    <textarea class="form-control" rows="3" cols="54" name="content"><%= list.content %></textarea>
</div>



<div class="form-group">
    <button type="submit" class="btn btn-success"> Create <i class="fa fa-database"></i>     
    </button>

</form>
</div>